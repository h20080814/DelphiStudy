unit UnitDomain;

interface

uses
  System.Classes;

type
  TProduct = class     //²úÆ·Àà
  private
    FId: Integer;
    FName: string;
    FIsConsumption: Boolean;
  public
    property Id: Integer read FId write FId;
    property Name: string read FName write FName;
    property IsConsumption: Boolean read FIsConsumption write FIsConsumption;
  end;

implementation

end.

