program Project1;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;
 {多维数组
    声明一个动态的二维数组
    type
      TStringDynamicArray = array of array of string;
    声明一个静态的二维数组
    type
    TStingStaticArray = array[0..3] of array[0..4] of string;

 }

type
  TStringDynamicArray = array of array of string;

type
  TStingStaticArray = array[0..3] of array[0..4] of string;

begin

  Readln;
end.

